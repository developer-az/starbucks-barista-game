<svg width="512" height="512" viewBox="0 0 512 512" xmlns="http://www.w3.org/2000/svg">
  <defs>
    <linearGradient id="gradient" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#006241;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#00754A;stop-opacity:1" />
    </linearGradient>
  </defs>
  
  <!-- Background circle -->
  <circle cx="256" cy="256" r="240" fill="url(#gradient)" stroke="#004C3F" stroke-width="4"/>
  
  <!-- Coffee cup -->
  <g transform="translate(256, 256)">
    <!-- Cup body -->
    <path d="M-80 -20 L-80 80 Q-80 100 -60 100 L60 100 Q80 100 80 80 L80 -20 Z" fill="#F5F5F0" stroke="#004C3F" stroke-width="3"/>
    
    <!-- Coffee -->
    <path d="M-75 -15 L-75 75 Q-75 95 -55 95 L55 95 Q75 95 75 75 L75 -15 Z" fill="#5B4636"/>
    
    <!-- Coffee foam -->
    <ellipse cx="0" cy="-15" rx="75" ry="8" fill="#F5F5F0"/>
    
    <!-- Handle -->
    <path d="M80 10 Q110 10 110 40 Q110 70 80 70" fill="none" stroke="#F5F5F0" stroke-width="6"/>
    <path d="M80 10 Q110 10 110 40 Q110 70 80 70" fill="none" stroke="#004C3F" stroke-width="3"/>
    
    <!-- Steam -->
    <g stroke="#F5F5F0" stroke-width="2" fill="none" opacity="0.7">
      <path d="M-30 -40 Q-25 -50 -30 -60 Q-35 -70 -30 -80">
        <animate attributeName="opacity" values="0.7;0.3;0.7" dur="2s" repeatCount="indefinite"/>
      </path>
      <path d="M0 -45 Q5 -55 0 -65 Q-5 -75 0 -85">
        <animate attributeName="opacity" values="0.3;0.7;0.3" dur="2s" repeatCount="indefinite" begin="0.5s"/>
      </path>
      <path d="M30 -40 Q35 -50 30 -60 Q25 -70 30 -80">
        <animate attributeName="opacity" values="0.7;0.3;0.7" dur="2s" repeatCount="indefinite" begin="1s"/>
      </path>
    </g>
    
    <!-- Star accent -->
    <polygon points="0,-100 3,-92 12,-92 6,-86 8,-77 0,-82 -8,-77 -6,-86 -12,-92 -3,-92" fill="#C8A27A"/>
  </g>
</svg>